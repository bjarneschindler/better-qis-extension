{
  "manifest_version": 3,
  "name": "Better QIS Extensions",
  "description": "Quality of life improvements for the QIS of the Kiel University of Applied Sciences",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["https://qis.fh-kiel.de/qisserver/rds?state=notenspiegel*"],
      "js": ["injected.js"],
      "css": ["injected.css"],
      "run_at": "document_end"
    }
  ],
  "permissions": ["activeTab"]
}
